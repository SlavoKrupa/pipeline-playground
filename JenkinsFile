node('maven') {
   // Mark the code checkout 'stage'....
   stage 'Checkout'

   // Get some code from a GitHub repository
   git url: 'https://github.com/LableOrg/java-maven-junit-helloworld.git'

   // Mark the code build 'stage'....
   stage 'Build'
   // Run the maven build
   sh "mvn clean install"
   stage 'Unit test'
   // Run the maven build
   sh "mvn test"
   stage 'Integration test'
   // Run the maven build
   sh "mvn verify"
   stage 'Check if configuration present'
   sh  "cat configuration/cicd-settings.xml"

}
